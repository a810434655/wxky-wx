/**app.wxss**/
.blur {
  filter: url(blur.svg#blur); /* FireFox, Chrome, Opera */
  -webkit-filter: blur(5px); /* Chrome, Opera */
  -moz-filter: blur(5px);
  -ms-filter: blur(5px);
  filter: blur(5px);
  filter: progid:DXImageTransform.Microsoft.Blur(PixelRadius=10, MakeShadow=false); /* IE6~IE9 */
}
/* reset */

page {
  background-color: #fff;
  height: 100%;
  font-size: 32rpx;
  line-height: 1.45;
  /* font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif; */
  font-family: YouYuan;
}

checkbox, radio {
  margin-right: 10rpx;
}

button {
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

form {
  width: 100%;
}

input {
  color: #4a505f;
}

.input-placeholder {
  color: #b3bdc9;
  font-size: 28rpx;
}

textarea {
  color: #4a505f;
}

.textarea-placeholder {
  color: #bbbec4;
}

/* lib */

.strong {
  font-weight: bold;
}

.tc {
  text-align: center;
}

.tr {
  text-align: right;
}
.mb-20 {
  margin-bottom: 20rpx;
}
.mb-30 {
  margin-bottom: 30rpx;
}
.pd-40{
  padding: 40rpx 0rpx
}
.pt-10{
  padding-top: 10rpx
}

.newlines {
  overflow: hidden;
  text-overflow: ellipsis;
}

.ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  -webkit-line-clamp:1;  /* 设置行数 */
}

.two-line {
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  align-content: center;
}
.four-line {
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 4;
  -webkit-box-orient: vertical;
  align-content: center;
}
.text-all {
  word-break: break-all;
}

/* radio */
radio .wx-radio-input {
  border: 12rpx solid #dde4ed;
  border-radius: 30rpx;
  width: 15rpx!important;
  height: 15rpx!important;
  background: transparent !important;
}
radio .wx-radio-input.wx-radio-input-checked::before {
  /* 自定义样式.... */
  border: 0rpx solid #ffbf00 !important;
  border-radius: 40rpx;
  color: transparent;
  background: transparent !important;
  /* position: absolute; */
}
radio .wx-radio-input-checked{
  background: transparent !important;
  border: 12rpx solid #ffbf00 !important;
  border-radius: 40rpx!important;
  width: 15rpx!important;
  height: 15rpx!important;
}

.checkbox{
  background: #f5f8fd;
  font-size: 28rpx;
  padding: 15rpx 20rpx;
  width: 155rpx;
  margin-bottom: 15rpx;
  color:#b3bdc9;
  text-align: center;
  border-radius: 4rpx;
  border:1px solid #dde4ed;
}
.checkbox.checked{
  background: rgba(90, 144, 255, 0.2);
  padding: 15rpx 20rpx;
  width: 155rpx;
  color: #051c33;
  text-align: center;
  border-radius: 4rpx;
  border:1px solid #286eff;
}

checkbox .wx-checkbox-input {
  border-radius: 0rpx;
  border: 0px;
  margin-top: -4rpx;
  width: 0px;
  height: 0px;
}


/* checkbox选中时样式 */

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  /* 自定义样式.... */
  padding: 0px;
  width: 0rpx;
  text-align: center;
  border-radius: 4rpx;
  border:0px;
  color:transparent; /* 对勾颜色 白色 */
  /* position: absolute; */
  background: transparent;
}

/* page */

.container {
  display: flex;
  flex-direction: column;
  min-height: 100%;
  justify-content: space-between;
  font-size: 32rpx;
}

.page-body {
  width: 100%;
  flex-grow: 1;
  overflow-x: hidden;
}

.page-content {
  padding: 10rpx 40rpx 40rpx 40rpx;
  width: auto;
}

.modal-content {
  padding: 30rpx 0rpx;
  width: auto;
}

swiper-item image {
  height: 78%;
  width: 93%;
}

.page-section {
  width: 100%;
}

.page-body-wrapper {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}

.bottom-shadow {
  box-shadow: 0px 4px 14px rgba(179, 189, 201, 0.2);
}

/* UI style */

.center {
  text-align: center;
  vertical-align: middle;
}

.centerv {
  display: flex;
  justify-content: center;
}
.centerv-c {
  display: flex;
  /* 主轴对齐模式 */
  justify-content: center;
  flex-direction:column;
  /* 交叉轴对齐模式 flex-start | flex-end | center | baseline | stretch */
  align-items:center;
}

.vertical-center {
  align-items: center;
  display: flex;
}

.all-center {
  display: flex;
  align-items: center;
  justify-content: center;
}

.circle {
  border-radius: 10rpx;
}

.radius {
  border-radius: 10rpx;
}
.radius-20 {
  border-radius: 20rpx;
}

.inline {
  display: inline-block;
}

/*  */
.top {
  width: 100%;
  height: 65rpx;
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  justify-content: space-between;
  background-color: #fff;
  z-index: 10;
}
.bottom {
  width: 100%;
  height: 130rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  justify-content: space-between;
  background-color: #fff;
  z-index: 10;
  box-shadow: 0 0 10rpx #c3c3c3;
}

.bottom button {
  box-shadow: 0 0 0rpx #c3c3c3;
}

